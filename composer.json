{
    "name": "poing/laravel-elb",
    "description": "Essentials for Laravel on AWS Elastic Beanstalk with HTTPS",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian LaVallee",
            "email": "brian.lavallee@invite-comm.jp"
        }
    ],
    "require": {
        "ocramius/package-versions": "^1.4",
        "czproject/git-php": "^3.17"
    },
    "autoload": {
        "psr-4": {
          "Poing\\Beanstalk\\": "source/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Beanstalk\\Test\\": "test/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Poing\\Beanstalk\\BeanstalkProvider"
            ]
        }
    }
}
